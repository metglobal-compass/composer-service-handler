{
  "name": "metglobal/composer-service-handler",
  "description": "Composer script handling for generating service definitions from class annotation",
  "type": "library",
  "keywords": [
    "annotations",
    "dependency injection",
    "service injection"
  ],
  "require": {
    "php": ">=7.0",
    "doctrine/annotations": "^1.4",
    "symfony/yaml": "~2.1|~3.4",
    "symfony/finder": "^2.8|~3.4"
  },
  "require-dev": {
    "composer/composer": "1.0.*@dev",
    "phpunit/phpunit": "5.*"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "Metglobal\\ServiceHandler\\": ""
    }
  },
  "autoload-dev":  {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Seyfi Aslan",
      "email": "seyfiaslangecinen@gmail.com"
    },
    {
      "name": "Burak Bolat",
      "email": "burakbolat@outlook.com"
    }
  ]
}
