{
  "name": "metglobal/composer-service-handler",
  "description": "Composer script handling for generating service definitions from class annotation",
  "type": "library",
  "keywords": [
    "annotations",
    "dependency injection",
    "service injection"
  ],
  "require": {
    "php": ">=7.0",
    "doctrine/annotations": "^1.4",
    "symfony/yaml": "^3.4|^4.4",
    "symfony/finder": "^3.4|^4.4"
  },
  "require-dev": {
    "composer/composer": "^1.0",
    "phpunit/phpunit": "^5.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "Metglobal\\ServiceHandler\\": ""
    }
  },
  "autoload-dev":  {
    "psr-4": {
      "Tests\\": "tests/",
      "TestBundle\\": "tests/Functional/Bundle/TestBundle",
      "Acme\\TestBundle\\": "tests/Functional/Bundle/Acme/TestBundle"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Seyfi Aslan",
      "email": "seyfiaslangecinen@gmail.com"
    },
    {
      "name": "Burak Bolat",
      "email": "brkblt@gmail.com"
    }
  ]
}
